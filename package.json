{
  "name": "korunka5-frontend-broiler-plate",
  "version": "0.5.0",
  "description": "Boilerplate pro Webpack Angular2 SASS aplikace",
  "scripts": {
    "postinstall": "npm run typings",
    "start:dev": "concurrently --kill-others \"npm run start:dev:api\" \"npm run start:dev:backoffice\" \"npm run start:dev:client\"",
    "start:dev:api": "echo \"DEV API run not yet implemented\" && exit 1",
    "start:dev:backoffice": "echo \"DEV API run not yet implemented\" && exit 1",
    "start:dev:client": "webpack-dev-server --inline --color --display-error-details --display-cached",
    "build:dev": "webpack --inline --color --display-error-details --display-cached",
    "build:test": "echo \"Not yet implemented\" && exit 1",
    "build:prod": "echo \"Not yet implemented\" && exit 1",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korunka/korunka5-frontend-broiler-plate.git"
  },
  "keywords": [
    "boilerplate",
    "korunka",
    "angular2",
    "sass",
    "webpack",
    "typescript"
  ],
  "author": "Ondra Hlaváč <ondra@hlavac.cz> (http://ondra.hlavac.cz)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/korunka/korunka5-frontend-broiler-plate/issues"
  },
  "homepage": "https://github.com/korunka/korunka5-frontend-broiler-plate#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.13",
    "bootstrap": "^3.3.6",
    "core-js": "^2.2.1",
    "ng2-bootstrap": "^1.0.7",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "^0.6.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.16.0-rc.0",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "0.35.0",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.10.0",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.3",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.8.3"
  }
}
